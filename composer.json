{
    "name": "2bo/stride",
    "description": "Explicit, Lightweight, Production-minded PHP framework",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Stride\\": "system/src/",
            "App\\": "app/"
        },
        "files": [
            "system/src/Helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}